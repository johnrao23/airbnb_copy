{
  "compilerOptions": {
    "strict": false,
    "checkJs": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "allowJs": true,
    "module": "commonjs",
    "target": "esnext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "*": ["src/*"]
    }
  },
  "exclude": ["node_modules"],
  "include": ["src", "typings"]
}


